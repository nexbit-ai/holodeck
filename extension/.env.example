# Plasmo Environment Variables
# Copy this file to .env and fill in your values

# Backend API endpoint
PLASMO_PUBLIC_API_URL=https://api.nexbit.io

# Login page URL
PLASMO_PUBLIC_LOGIN_URL=https://app.nexbit.io/login

# App base URL (for editor redirect)
PLASMO_PUBLIC_APP_URL=https://app.nexbit.io

# Dev token for testing (optional, only for local development)
PLASMO_PUBLIC_DEV_TOKEN=
